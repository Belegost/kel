                <div class="big-digits">{{ totalInvestedFunds|number_format(2, '.', '') }}</div>
                {#                <p>BTC invested in products and equivalent</p>#}
            </div>
        </div>

        <div class="bg-white box-framed bg-white dashboard-charts-cryptoassets">
